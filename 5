    AREA CONSTANT, DATA, READONLY
NUM1    EQU 0x0040
NUM2    EQU 0x0010

    AREA MYCODE, CODE, READONLY
    ENTRY
    EXPORT __main

__main
    MOV R0, #NUM1        ; R0 = 0x40 (64)
    MOV R1, #NUM2        ; R1 = 0x10 (16)
    MUL R2, R1, R0       ; R2 = R1 * R0 = 0x400 (1024)

HERE
    B HERE               ; Infinite loop

    END
