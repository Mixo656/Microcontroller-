    AREA LOGIC, CODE, READONLY
    ENTRY
    EXPORT __main

__main
    LDR R0, =0x0A5A5    ; R0 = 0000 1010 0101 1010 0101
    LDR R1, =0xFAF56    ; R1 = 1111 1010 1111 0101 0110

    AND R3, R0, R1     ; R3 = 0000 1010 0101 0000 0100 = 0xA504
    ORR R4, R0, R1     ; R4 = 1111 1010 1111 1111 0111 = 0xFAFF7
    EOR R5, R0, R1     ; R5 = 1111 0000 1010 1111 0011 = 0xF0AF3
    BIC R6, R0, R1     ; R6 = 0000 0000 0000 1010 0001 = 0xA1

STOP
    B STOP             ; Infinite loop (halt)

    END
