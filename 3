    AREA MYCODE, CODE, READONLY
    ENTRY
    EXPORT __main

__main
START
    LDR R0, =0x112        ; Load 0x112 (decimal 274) into R0
    LDR R1, =0x341        ; Load 0x341 (decimal 833) into R1
    MUL R2, R1, R0        ; R2 = R1 * R0 = 0x341 * 0x112

HERE
    B HERE                ; Infinite loop

    END
